{
  "name": "@js-bits/formalinter",
  "version": "1.0.1",
  "description": "Add formatting and linting to your project in one simple step",
  "keywords": [
    "javascript",
    "linting",
    "tooling",
    "formatting",
    "vscode",
    "eslint",
    "prettier"
  ],
  "scripts": {
    "test": "echo \"Nothing to test here\"",
